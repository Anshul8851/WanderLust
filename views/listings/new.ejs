<% layout("/layouts/boillerplate") %>
<body>
    <h1>Enter Details Of New Listing</h1>
    <form method="POST" action="/listings">
        <label for="title" >Title:</label>
        <input type="text" name="title" id="title" placeholder="enter title"/>
        <br/><br/>
        <label for="description" >Description:</label><br/><br/>
        <textarea name="description" id="description"placeholder="enter description"></textarea>
        <br/><br/>
        <label for="country" >Country:</label>
        <input type="text" name="country" id="country" placeholder="enter country"/>
        <br/><br/>
        <label for="location" >Location:</label>
        <input type="text" name="location" id="location" placeholder="enter location"/>
        <br/><br/>
        <label for="price" >Price:</label>
        <input type="number" name="price" id="price" placeholder="enter price"/>
        <br/><br/>
        <label for="image" >Image Url:</label>
        <input type="text" name="image" id="image" placeholder="enter img url"/>
        <br/><br/>
        <button>Submit Details</button>
       
    </form>
</body>
